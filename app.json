{
	"expo": {
		"name": "FleetManagement",
		"slug": "FleetManagement",
		"assetBundlePatterns": ["**/*"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.gamemine.fleet",
			"buildNumber": "1.0.0",
			"config": {
				"googleMapsApiKey": "AIzaSyChB1cQ5QSIhAqSuAhjAWn4qOjqEXo3j_w"
			}
		},
		"android": {
			"icon": "./assets/icon.png",
			"package": "com.gamemine.fleet",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"config": {
				"googleMaps": {
					"apiKey": "AIzaSyDn3LHUTSWV7woXQdjTOt5BaBYQUdQCZgo"
				}
			},
			"userInterfaceStyle": "light",
			"googleServicesFile": ""
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-location",
				{
					"locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
				}
			],
			[
				"expo-local-authentication",
				{
					"faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
				}
			],
			[
				"expo-sqlite",
				{
					"enableFTS": true,
					"useSQLCipher": true,
					"android": {
						"enableFTS": false,
						"useSQLCipher": false
					},
					"ios": {
						"customBuildFlags": ["-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"]
					}
				}
			],
			[
				"expo-asset",
				{
					"assets": ["./assets/test_uurhai.kml", "./assets/"]
				}
			]
		]
	}
}
